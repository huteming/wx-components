<app-header title="{{ title }}" />
<app-checkbox name="checkbox" options="{{ options }}" catch:change="handleChange" />


<!--  文档  -->
<app-header title="属性" />
<app-content>
  <app-cell label="name" value="标识符" />
  <app-cell label="value*Array" value="初始值" />
  <app-cell label="options*Array" value="选项, {label, value}" />
</app-content>

<app-header title="事件" />
<app-content>
  <app-cell label="change" value="{name, value}" />
</app-content>